#define __PIO_FILE__ "pionfget_mod.F90"
!>
!! @file
!! @brief Read Routines for non-decomposed NetCDF data.
!<
module pionfget_mod
  use iso_c_binding
#ifdef TIMING
  use perf_mod, only : t_startf, t_stopf      ! _EXTERNAL
#endif
